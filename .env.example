# DigiLocker OAuth2 Configuration
DIGILOCKER_CLIENT_ID=your_client_id_here
DIGILOCKER_CLIENT_SECRET=your_client_secret_here

# Callback URL for OAuth2 flow
# In development: http://localhost:3000/api/auth/callback
# In production: https://your-domain.com/api/auth/callback
DIGILOCKER_REDIRECT_URI=http://localhost:3000/api/auth/callback

# Node Environment
NODE_ENV=development